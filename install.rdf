<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
		xmlns:em="http://www.mozilla.org/2004/em-rdf#">

	<Description about="urn:mozilla:install-manifest">
		<em:id>helloworld@zotero.org</em:id>
		<em:name>Zotero Redownloader</em:name>
		<em:version>0.0.1</em:version>
		<em:description>Download missing attachments from url</em:description>
		<em:creator>elbaro</em:creator>
		<!-- <em:updateURL>https://www.zotero.org/download/plugins.rdf</em:updateURL> -->
		<em:type>2</em:type> <!-- type=extension -->

		<!-- Requires Zotero -->
		<em:requires>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:name>Zotero</em:name>
				<em:homepageURL>http://www.zotero.org</em:homepageURL>
				<em:minVersion>5.0.0</em:minVersion>
				<em:maxVersion>5.*</em:maxVersion>
			</Description>
		</em:requires>

		<em:targetApplication>
			<Description>
				<em:id>zotero@chnm.gmu.edu</em:id>
				<em:minVersion>5.0.0</em:minVersion>
				<em:maxVersion>5.0.*</em:maxVersion>
			</Description>
		</em:targetApplication>
	</Description>
</RDF>
